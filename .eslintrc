{
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/forbid-prop-types": [0, { "forbid": ['any', 'array', 'object'] }],
    "max-len": [1, {"code": 150}],
    "no-underscore-dangle": [0],
    "new-cap": [0],
  }
}
